{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-debugger": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [0, "single"],
    "jsx-quotes": [1, "prefer-double"],
    "react/jsx-no-undef": 0,
    "react/jsx-uses-react": 0,
    "react/jsx-uses-vars": 0,
    "no-unused-vars": 0,
    "no-case-declarations": 0
  }
}